        <body>

            <main class="main">

                <section class="pt-150 pb-150">

                    <div class="container">

                        <div class="row">

                            <div class="col-lg-10 m-auto">
                                <div class="row">
                                    <h2>My Account</h2>
                                    <br>
                                    <br>
                                    <div class="col-md-4">
                                        <div class="dashboard-menu">
